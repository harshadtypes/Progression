{
    "version": 2,
    "builds": [
        {
            "src":"./index.py",
            "use": "@vercel/python",
            "config":{"runtime":"Python3.11.4"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"app.py"
        }
    ],

  "installCommand": "pip install -r requirements.txt"
}