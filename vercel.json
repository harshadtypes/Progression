{
    "version": 2,
    "builds": [
        {
            "src":"./index.py",
            "use": "@vercel/python",
            "config":{"runtime":"Python3.12.x"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"app.py"
        }
    ],
    "buildCommand": "./build.sh"
}